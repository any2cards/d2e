// WSHRemoveCommaAtEndOfLine: Remove Comma At End Of Specific Line

macro WSHRemoveCommaAtEndofLine(void)
{
	int i;
	
	for (i = 1; i <= LineCount; i++)
	{
		if (LineContains(i,"\"cardno\": ") > 0)
		{
			GotoLine(i);
			EndofLine;
			Backspace;
		}
	}
	
	Gotoline(1);
}






































































































































































































































































































































































































































































































