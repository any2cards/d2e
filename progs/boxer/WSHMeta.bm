// WSHMeta: Add Meta Values Where Appropriate

macro WSHMeta(void)
{
	int i;

	for (i = 1; i <= LineCount; i++)
	{
		if (LineContains(i,"\"cardno\": ") > 0)
		{
			GotoLine(i);
			StartofLine;
//			if (LineContains(i+1,"\"act\": ") > 0)
//			{
//				continue;
//			}
			InsertLineBelow;
			GotoLine(i+1);
			printf("    \"flavor\": \"xxxxx\",\n");
			printf("    \"effects\": \"xxxxx\",\n");
			printf("    \"icons\": \"xxxxx\",");
		}
	}
	
	Gotoline(1);
}















































































































































































































































































































































































