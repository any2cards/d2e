// WSHExpansion: Insert Expansion Value After Points Value

macro WSHExpansion(void)
{
	int i;
	int j;
	
	j=0;

	for (i = 1; i <= LineCount; i++)
	{
		if (LineContains(i,"\"points\": ") > 0)
		{
			GotoLine(i);
			StartofLine;
			InsertLineBelow;
			GotoLine(i+1);
			printf("    \"expansion\": \"Base Game\",");
			++j;
		}
	}
	
	Gotoline(1);
}















































































































































































































































































































































































