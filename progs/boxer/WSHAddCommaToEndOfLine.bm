// WSHAddCommaToEndOfLine: Add Comma To End Of Specific Line

macro WSHAddCommaToEndofLine(void)
{
	int i;
	
	for (i = 1; i <= LineCount; i++)
	{
		if (LineContains(i,"\"cardno\": ") > 0)
		{
			GotoLine(i);
			EndofLine;
			printf(",");
		}
	}
	
	StartOfFile;
	Save;
}















































































































































































































































































































































































































































































































































