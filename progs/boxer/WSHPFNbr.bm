// WSHPFNbr: Changes End of Line Printflag Number

macro WSHPFNbr(void)
{
	int i;
	string pf;
	
	GetString("Enter Print Flag Value: ",pf,"#");
	strlwr(pf);
	

	for (i = 1; i <= LineCount; i++)
	{
		if (LineContains(i,"-back.png") > 0)
		{
			GotoLine(i);
			EndofLine;
			Backspace;
			printf("%s",pf);
		}
	}
	
	Gotoline(1);
}












































































































































































































































































































































































































































































































































































































































































































































































































































































































