// WSHZZJSONPoints: Re-Number JSON File Points Value

macro WSHZZJSONPoints(void)
{
	int i;
	int j;
	
	j=0;

	GetInt("Enter Starting Points Value: ",j);

	for (i = 1; i <= LineCount; i++)
	{
		if (LineContains(i,"\"points\": ") > 0)
		{
			GotoLine(i);
			StartofLine;
			DeleteToEndOfLine;
			printf("    \"points\": %i,",j);
			++j;
		}
	}
	
	Gotoline(1);
	StartOfLine;
	Save;
}














































































































































































































































































































































































































































































































































































































































































































































































































































































































































































